{
  "extends": "expo/tsconfig.base",

  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts"
, "app/(public)/reset.jsx", "app/(public)/register.jsx", "app/(public)/register.jsx", "app/(public)/register.jsx"  ],
  "compilerOptions": {
    "strict": true,
    "paths": {
      "@/*": [
        "./*"
      ]
    },
    "target": "esnext",
    "module": "commonjs",
    "jsx": "react-native",
    
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  },
  "exclude": [
    "node_modules",
    "babel.config.js",
    "metro.config.js",
    "jest.config.js"
  ]
}
